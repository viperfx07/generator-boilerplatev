{
  "name": "<%= projectName.replace(/ /ig,'') %>",
  "version": "0.1.0",
  "devDependencies": {
    "browser-sync": "latest",
	"del": "latest",
	"emitty": "1.4.0",
	"eslint": "^5.5.0",
	"eslint-plugin-import": "^2.13.0", <% if (jsFramework === 'React') { %>
	"eslint-plugin-jsx-a11y": "^6.1.1",
	"eslint-plugin-react": "^7.10.0", <% } else { %>
	"eslint-plugin-vue": "^4.7.1",
	"file-loader": "^3.0.1", <% } %>
	"foldero": "latest",
	"gulp-data": "latest",
	"gulp-pug": "latest",
	"gulp-watch": "latest", <% if (jsFramework === 'Vue') { %>
	"import-glob-loader2": "^1.1.2",
	"mini-css-extract-plugin": "^0.5.0", <% } %>
	"prettier-eslint": "^8.8.2",
	"pug": "latest",
	"pug-include-glob": "0.3.3", <% if (jsFramework === 'Vue') { %>
	"resolve-url-loader": "^3.0.0",
	"vue-eslint-parser": "^3.2.2",
	"vue-styleguidist": "^3.0.0",
	"webfonts-loader": "^4.3.0" <% } %>
  }, <% if (jsFramework === 'Vue') { %>
  "scripts": {
  	"styleguide": "vue-styleguidist server",
  	"styleguide:build": "vue-styleguidist build"
  }, <% } %>
  "//": "CUSTOM CONFIGURATION",
  "config": {
    "//": "Entry files",
    "host": "127.0.0.1",
    "port": "3000",
    "baseUrl": "./",
    "directories": {
      "source": "src",
      "destination": "www",
      "otherWWW": [ "<%= otherWWW %>" ],
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "assets": "<%= assetsDir %>",
      "modules": "_modules",
      "layouts": "_layouts",
      "images": "_img",
      "icons": "_icons",
      "fonts": "_fonts",
      "styles": "_css",
      "scripts": "_js",
      "data": "_data"
    },
    "entries": {
      "js": "main.js",
      "css": "main**.{sass,scss}"
    }
  },
  "dependencies": {
	"@babel/core": "^7.0.1",
	"@babel/plugin-proposal-class-properties": "^7.0.0",
	"@babel/plugin-proposal-decorators": "^7.0.0",
	"@babel/plugin-syntax-dynamic-import": "^7.0.0",
	"@babel/plugin-transform-runtime": "^7.0.0",
	"@babel/preset-env": "^7.0.0", <% if (jsFramework === 'React') { %>
	"@babel/preset-react": "^7.0.0", <% } %>
	"@babel/register": "^7.0.0",
	"@babel/runtime": "^7.0.0",
	"autoprefixer": "latest",
	"axios": "^0.18.0",
	"babel-eslint": "^8.2.6",
	"babel-loader": "^8.0.2",
	"babel-plugin-add-module-exports": "^1.0.0",
	"bootstrap": "^4.1.1",
	"clamp-js-main": "^0.11.4", <% if (jsFramework === 'React') { %>
	"classnames": "^2.2.6", <% } %>
	"conditioner-core": "^2.3.1", <% if (jsFramework === 'Vue') { %>
	"css-loader": "^2.1.0", <% } %>
	"date-fns": "^1.29.0",
	"feature.js": "^1.0.0",
	"glob": "^7.1.3",
	"gulp": "^4.0.2",
	"gulp-changed": "latest",
	"gulp-size": "latest",
	"gulp-consolidate": "latest",
	"gulp-css-globbing": "latest",
	"gulp-cssnano": "latest",
	"gulp-eslint": "latest",
	"gulp-iconfont": "latest",
	"gulp-if": "latest",
	"gulp-load-plugins": "latest",
	"gulp-notify": "latest",
	"gulp-plumber": "latest",
	"gulp-postcss": "latest",
	"gulp-rename": "latest",
	"gulp-replace": "latest",
	"gulp-sass": "latest",
	"gulp-sourcemaps": "latest",
	"hamburgers": "latest",
	"hash-files": "^1.1.1",
	"headroom.js": "^0.9.4",
	"instafeed.js": "^1.4.1",
	"jquery.scrollto": "^2.1.2",
	"json5": "^0.5.1",
	"lazysizes": "^4.0.1",
	"lodash": "latest",
	"magnific-popup": "^1.1.0",
	"minimist": "^1.2.0",
	"popper.js": "^1.14.3",
	"postcss-critical-split": "latest",
	"postcss-pxtorem": "latest", <% if (jsFramework === 'Vue') { %>
	"pug-plain-loader": "^1.0.0", <% } %>
	"raf-throttle": "^2.0.3", <% if (jsFramework === 'React') { %>
	"react-loadable": "^5.5.0",
	"recompose": "^0.27.1", <% } %>
	"rucksack-css": "latest",
	"run-sequence": "latest", <% if (jsFramework === 'Vue') { %>
	"sass-loader": "^7.1.0", <% } %>
	"simplestatemanager": "latest",
	"slick-carousel": "latest",
	"smoothscroll-for-websites": "^1.4.6",
	"sprintf-js": "^1.1.1", <% if (jsFramework === 'Vue') { %>
	"style-loader": "^0.23.1", <% } %>
	"twitter-fetcher": "^18.0.2",
	"uglifyjs-webpack-plugin": "^1.3.0", <% if (jsFramework === 'Vue') { %>
	"vue": "^2.5.21",
	"vue-loader": "^15.4.2",
	"vue-style-loader": "^4.1.2",
	"vue-template-compiler": "^2.5.21", <% } %>
	"webpack": "^4.19.0",
	"webpack-stream": "viperfx07/webpack-stream",
	"wrench": "latest",
	"wurl": "^2.5.3"
  },
  "resolutions":{
	 "chokidar": "2.0.0",
	 "watchpack": "1.4.x",
	 "natives": "^1.1.3",
	 "lodash.template": "^4.5.0",
	 "handlebars": "^4.1.2"
  }
}
