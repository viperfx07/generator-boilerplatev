  "//": "CUSTOM CONFIGURATION",
  "config": {
    "//": "Entry files",
    "host": "127.0.0.1",
    "port": "3000",
    "baseUrl": "./",
    "directories": {
      "source": "src",
      "destination": "www",
      "otherWWW": [ "<%= otherWWW %>" ],
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "assets": "<%= assetsDir %>",
      "modules": "_modules",
      "layouts": "_layouts",
      "images": "_img",
      "icons": "_icons",
      "fonts": "_fonts",
      "styles": "_css",
      "scripts": "_js",
      "data": "_data"
    },
